<%= render 'layouts/navbar' %>
<div class="body">
	<div class="left"><%= link_to "Back to #{@artist.name}", billboard_artist_path(@artist.billboard_id,@artist) %></div>
	<h1><%= @artist.name %> Songs</h1>
	<%= link_to 'Add Song', new_artist_song_path(@artist) %>
	<ul class="collection">
		<% @songs.each do |song| %>
			<li class="collection-item">
				<%= link_to song.name, [@artist, song] %>
			</li>
		<% end %>
	</ul>
</div>