<%= render 'layouts/navbar' %>
<div class="body">
	<div class="left"><%= link_to "Back to #{@billboard.name}", billboard_path(@billboard) %></div>
	<h1>Artists</h1>
	<%= link_to 'Create Artist', new_billboard_artist_path(@billboard) %>
	<ul class="collection">
		<% @artists.each do |artist| %>
			<li class="collection-item">
				<%= link_to artist.name, [@billboard, artist] %>
			</li>
		<% end %>
	</ul>
	<br />
</div>