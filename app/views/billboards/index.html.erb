<%= render 'layouts/navbar' %>
<div class="body">
	<h1>Top Billboards!</h1>
	<%= link_to 'Create a new billboard', new_billboard_path %>
	<ul class="collection">
		<% @billboards.each do |billboard| %>
			<li class="collection-item">
			<%= link_to billboard.name, billboard %>
			</li>
		<% end %>
	</ul>
</div>